#include <stdio.h>

int main(int argc, char *argv[])
{
  if(argc < 7)
  {
    printf("\n"
           "FFTWTEST Test for SMP FFTW speed - (C) 2010 Li Chaozheng\n"
           "Note: you must have compiled FFTW with './configure --enable-threads'\n"
           "\n"
           "fftwtest <l> <n> <i> <s> <r> <f>\n"
           " l : number of FFT points\n"
           " n : number of SMP threads\n"
           " i : how many FFT calls to make and average their exec times\n"
           " s : source filename\n"
           " r : result filename\n"
           " f : log filename\n\n");
    return -1;
  }

  return 0;
}
